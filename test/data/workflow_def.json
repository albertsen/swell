{
    "id": "fulfilorder",
    "description": "Example order fulfilment process",
    "status": "PLACED",
    "actionactionrs": {
        "start": {
            "type": "endpoint",
            "url": "http://loalhost:3010/action?orderStatus=CREATED&event=started"
        },
        "checkOrder": {
            "type": "endpoint",
            "url": "http://loalhost:3010/action?orderStatus=CHECKED&event=orderChecked"
        },
        "capturePayment": {
            "type": "endpoint",
            "url": "http://loalhost:3010/action?orderStatus=PAYMENT_CAPTURED&event=paymentCaptured"
        },
        "prepareShipment": {
            "type": "endpoint",
            "url": "http://loalhost:3010/action?orderStatus=SHIPMENT_PREPARED&event=shipmentPrepared"
        },
        "ship": {
            "type": "endpoint",
            "url": "http://loalhost:3010/action?orderStatus=SHIPPED&event=shipped"
        },
        "error": {
            "type": "endpoint",
            "url": "http://loalhost:3010/action?orderStatus=ERROR&event=error"
        },
        "done": {
            "type": "endpoint",
            "url": "http://loalhost:3010/action?orderStatus=COMPLETED&event=done"
        }
    },
    "steps": {
        "start": {
            "started": "checkOrder"
        },
        "checkOrder": {
            "orderValid": "capturePayment"
        },
        "capturePayment": {
            "paymentCaptured": "prepareShipment"
        },
        "prepareShipment": {
            "shipmentPrepared": "ship"
        },
        "ship": {
            "shipped": "done"
        }
    }
}